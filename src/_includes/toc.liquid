<article class="post full">

  <div class="post-main">



    {% assign query = collections.all | where: "fileSlug", project | first %}

    <div class="post-content">
      <h2 class="project-header">contents</h2>

      {% assign contentsList = collections.all | fromFiles: contents %}
      <div class="post-content-text">
        {% for post in contentsList %}
        <a href="#{{ post.data.title | slug }}">{{ post.data.title }}</a><br/>
        {% endfor %}
      </div>

    </div>


  </div>

</article>
